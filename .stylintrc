{
    "blocks": {
        "expect": false,
        "error": true
    },
    "brackets": {
        "expect": "never",
        "error": true
    },
    "colons": {
        "expect": "never",
        "error": true
    },
    "colors": {
        "expect": "always",
        "error": true
    },
    "commaSpace": {
        "expect": "always",
        "error": true
    },
    "commentSpace": {
        "expect": "always",
        "error": true
    },
    "cssLiteral": {
        "expect": "never",
        "error": true
    },
    "customProperties": [],
    "depthLimit": {
        "expect": 3,
        "error": true
    },
    "duplicates": {
        "expect": false,
        "error": true
    },
    "efficient": {
        "expect": "always",
        "error": true
    },
    "exclude": [],
    "extendPref": {
        "expect": false,
        "error": true
    },
    "globalDupe": {
        "expect": false,
        "error": true
    },
    "groupOutputByFile": {
        "expect": true,
        "error": true
    },
    "indentPref": {
        "expect": 2,
        "error": true
    },
    "leadingZero": {
        "expect": "never",
        "error": true
    },
    "maxErrors": {
        "expect": false,
        "error": true
    },
    "maxWarnings": {
        "expect": false,
        "error": true
    },
    "mixed": {
        "expect": 2,
        "error": true
    },
    "mixins": [],
    "namingConvention": {
        "expect": "camelCase",
        "error": true
    },
    "namingConventionStrict": {
        "expect": true,
        "error": true
    },
    "none": {
        "expect": "never",
        "error": true
    },
    "noImportant": {
        "expect": "never",
        "error": true
    },
    "parenSpace": {
        "expect": false,
        "error": true
    },
    "placeholders": {
        "expect": "always",
        "error": true
    },
    "prefixVarsWithDollar": {
        "expect": "always",
        "error": true
    },
    "quotePref": {
        "expect": "single",
        "error": true
    },
    "reporter": "stylint-stylish",
    "reporterOptions": {
        "columns": ["lineData", "severity", "description", "rule"],
        "columnSplitter": "  ",
        "showHeaders": false,
        "truncate": true
    },
    "semicolons": {
        "expect": "never",
        "error": true
    },
    "sortOrder": {
        "expect": "alphabetical",
        "error": true
    },
    "stackedProperties": {
        "expect": "never",
        "error": true
    },
    "trailingWhitespace": {
        "expect": "never",
        "error": true
    },
    "universal": {
        "expect": false,
        "error": true
    },
    "valid": {
        "expect": true,
        "error": true
    },
    "zeroUnits": {
        "expect": "never",
        "error": true
    },
    "zIndexNormalize": {
        "expect": false,
        "error": true
    }
}
